<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lcdt.notify.dao.NotifySettingDao">


    <select id="selectByNotifyAndCompany" resultMap="com.lcdt.notify.dao.NotifySettingMapper.BaseResultMap">
        SELECT * from company_notify_setting
        where company_id = #{companyId}
        and notify_id = #{notifyId}
    </select>



</mapper>